<div class="card grey">

      <img class="card-img-top" 
      src={{this.image_url}} alt="">

      <div class="card-body">

        <h2 class="card-title">{{this.name}}</h2>
        <h6 class="card-subtitle mb-2 text-muted">{{this.tagline}}</h6>
        <p class="card-text">{{this.description}}</p>

      </div>
</div>
